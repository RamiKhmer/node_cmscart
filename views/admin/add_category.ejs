<%- include('../layouts/adminheader')%> 

<h2 class="page-title">Add Category</h2>

<a href="/admin/categories" class="btn btn-primary">Back to All Categories</a>
<br><br>

<form method="POST" action="/admin/categories/add-category">

    <div class="form-group">
        <label for="">Title</label>
        <input id="" class="form-control" type="text" name="title" value="<%= title %>" placeholder="Title">
    </div>

    <button class="btn btn-success">Submit</button>

</form>

<%- include('../layouts/adminfooter') %> 